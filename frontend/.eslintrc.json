{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "plugins": [
    "react-hooks",
    "react",
    "@typescript-eslint"
  ],
  "extends": [
    "prettier",
    "react/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error"
  }
}
