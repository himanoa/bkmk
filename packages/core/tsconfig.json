{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./lib/",
    "rootDir": "./src/",
    "baseUrl": "./",
    "target": "es5",
    "module": "commonjs",
    "declarationDir": "./lib",
    "noLib": false
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    ".git",
    "node_modules",
    "./src/*.spec.ts",
    "./src/*.test.ts"
  ]
}
